<%- include('partials/head') %>
  </head>

  <body>
    <%- include('partials/nav') %>
      <main class="container mx-auto bg-white shadow-lg rounded-lg p-8 mt-10 max-w-xl">
        <h2 class="text-3xl font-bold mb-6 text-center text-red-500">Here are our registered homes:</h2>
        <ul>
          <% registeredHomes.forEach(home=> { %>
            <li class="mb-4 p-4 bg-gray-100 rounded shadow hover:bg-gray-200 transition">
              <span class="text-lg font-semibold text-gray-800">
                <%= home.houseName %>
              </span>
            </li>
            <% })%>
        </ul>
      </main>
  </body>

  </html>